            </Grid>
        </GroupBox>
        <StackPanel HorizontalAlignment="Right" Name="stackPanel2" VerticalAlignment="Top" DockPanel.Dock="Right" Margin="15,0,0,0">
            <GroupBox Name="groupBox2">
                <StackPanel Name="stackPanel1">
                    <Button Content="Parar" Height="23" Name="buttonStartStop" Width="120" Margin="0" Click="buttonStartStop_Click" />
                    
                </StackPanel>
            </GroupBox>
            <GroupBox Header="Prova" Name="groupBox3">
                <StackPanel>
                    <Label Content="" Name="prova" />
                    <Label Content="" Name="item" />
                    <Label Content="" Name="item1" />
                    <Label Content="" Name="item2" />
                    <Label Content="" Name="item3" />
                    <Label Content="" Name="item4" />
                    <Label Content="" Name="item5" />
                    <Label Content="" Name="item6" />
                    <Label Content="" Name="item7" />
                    <Label Content="" Name="item8" />
                    <Label Content="" Name="item9" />
                    <Label Content="" Name="item10" />
					
					
                    <Label Content="" Name="refer" />
                </StackPanel>
            </GroupBox>
        </StackPanel>
    </DockPanel>
</Window>
